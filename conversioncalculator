//How to get a function to return a value

function setup() {
var km = milesToKm(26.3)
print(km);  
var km2 = milesToKm(100)
print(km2)
var stone = poundsToStone(190)
print(stone)

  createCanvas(400, 400);
  var angle = degrees(PI/2);
