//nestedloops
//adding a y variable inside x loop


function setup() {
  createCanvas(600, 400);
}

function draw() {
    background(0);
    stroke(255);
    strokeWeight(4);
    for  (var x = 0; x < width; x += 50) { //define variable, x       less than 0 and less than width
      for (var y = 0; y <= height; y += 50) {
    fill (random(255), 0, random(255)); // draw the ellipse
    ellipse(x, y, 25, 25); //draw the ellipse
  } 
    }
}
