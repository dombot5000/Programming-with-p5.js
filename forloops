//forloops


function setup() {
  createCanvas(600, 400);
}

function draw() {
    background(0);
    stroke(255);
    strokeWeight(4);
    for  (var x = 0; x < width; x = x + 50) { //define variable, x       less than 0 and less than width
    fill (255, 0, 200); // draw the ellipse
    ellipse(x, 100, 25, 25); //draw the ellipse
  } 
  
}
