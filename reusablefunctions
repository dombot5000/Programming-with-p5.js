function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  lollipop(100, 100, 60); //arguments have to be defined here
  lollipop(300, 190, 200);
}

function lollipop(x, y, diameter) {
  fill (0, 200, 255);
  rect(x-10, y, 20, 150);
  fill(255, 0 ,200);
  ellipse(x, y, diameter, diameter); 
}
