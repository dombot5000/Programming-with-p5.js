var nums = [100, 10, 133, 20]  // the index in the array is 0 1 2 3

function setup() {
  createCanvas(600, 400);
}

function draw() {
  background(0);
  
 for (var i = 0; i < 4; i++) { //i is the index number for the array!
 ellipse(i*100+100, 200, nums[i], nums[i]);  // we are using i to set the x coorinate AND for the ellipse radius

}
}
